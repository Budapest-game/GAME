{
  "name": "game",
  "version": "1.0.0",
  "description": "Собрать 3 и более одинаковых элемента в ряд.\r ## Шаблоны страниц  \r https://www.figma.com/file/Q4k80uj2TfDdDGHBdo5paa/Game?node-id=0%3A1 \r ## Механика\r Игровое поле размером N на N элементов.",
  "main": "server.js",
  "scripts": {
    "lint": "eslint --ext=.ts,.tsx, ./src/  ./server/  ./webpack/",
    "lint-fix": "eslint --ext=.ts,.tsx  --fix ./src/  ./server/  ./webpack/ ",
    "test": "jest -u",
    "test-watch": "jest -u --watch",
    "build-configs": "tsc --p webpack/tsconfig_webpack.json",
    "build-server": "tsc --p server/tsconfig_server.json ",
    "build-client-prod": "cross-env NODE_ENV=production webpack --config ./dist/webpack/webpack.client",
    "build-ssr-prod": "cross-env NODE_ENV=production webpack --config ./dist/webpack/webpack.ssr",
    "build-client-dev": "cross-env NODE_ENV=development webpack --config ./dist/webpack/webpack.client",
    "build-ssr-dev": "cross-env NODE_ENV=developments webpack --config ./dist/webpack/webpack.ssr",
    "build-dev": "npm run build-configs && npm run build-server && npm run build-ssr-dev && npm run build-client-dev ",
    "build-prod": "npm run build-configs && npm run build-server && npm run build-ssr-prod && npm run build-client-prod ",
    "start-dev": "cross-env NODE_ENV=development node ./dist/server/server.js",
    "start-prod": "cross-env NODE_ENV=production node ./dist/server/server.js ",
    "dev-server": "npm run build-server && cross-env NODE_ENV=development node ./dist/server/server.js",
    "dev": "npm run build-dev && npm run start-dev",
    "prod": "npm run build-prod && npm run start-prod"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Budapest-game/GAME.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Budapest-game/GAME/issues"
  },
  "engines": {
    "node": ">= 14.0.0",
    "npm": ">= 6.0.0"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/tests/__mocks__/fileMock.ts",
      "\\.(css|less)$": "<rootDir>/tests/__mocks__/styleMock.ts"
    },
    "setupFilesAfterEnv": [
      "@testing-library/jest-dom/extend-expect"
    ]
  },
  "homepage": "https://github.com/Budapest-game/GAME#readme",
  "dependencies": {
